{
    "name": "kickstart-angular-1",
    "title": "Kickstart",
    "description": "Kickstart ESP Demo for Angular 1",
    "version": "0.8.2",
    "keywords": [
        "appweb",
        "esp"
    ],
    "repository": {
        "type": "git",
        "url": "git://github.com/embedthis/kickstart-angular-1.git"
    },
    "skeletons": {
        "esp-angular-skeleton": "~5.5",
        "esp-angular-mgr-skeleton": "~5.5"
    },
    "dependencies": {
        "angular": "~1.4",
        "angular-animate": "~1.4",
        "angular-bootstrap": "~*",
        "angular-route": "~1.4",
        "bootstrap": "~3.3",
        "esp-angular": "~5.5",
        "esp-angular-best": "~*",
        "esp-angular-gui": "~*",
        "esp-mvc": "~*",
        "exp-angular": "~*",
        "exp-canon": "~*",
        "exp-css": "~*",
        "exp-esp": "~0.3",
        "exp-gzip": "~0.3",
        "exp-html": "~*",
        "exp-js": "~*",
        "exp-less": "~*",
        "font-awesome": "~4.3",
        "html5shiv": "~*",
        "jquery": "~*",
        "more": "~*",
        "respond": "~*"
    },
    "devDependencies": {
        "expansive": ">=0.7.0",
        "esp": ">=7.0"
    },
    "profile": "prod",
    "import": true,
    "overrides": {
        "bootstrap": {
            "export": [
                "less/**",
                {
                    "from": [
                        "dist/fonts/*.eot",
                        "dist/fonts/*.woff*",
                        "dist/fonts/*.ttf"
                    ],
                    "trim": 1
                }
            ]
        },
        "font-awesome": {
            "export": [
                "less/**",
                {
                    "from": [
                        "fonts/*.eot",
                        "fonts/*.woff*",
                        "fonts/*.ttf"
                    ]
                }
            ]
        }
    },
    "directories": {
        "export": "contents/lib",
        "dist": "dist",
        "controllers": "controllers"
    }
}
