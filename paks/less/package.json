{
    "name": "less",
    "description": "CSS pre-processor for the Less stylesheet language",
    "version": "1.3.3",
    "keywords": [
        "esp",
    ],
    "export": [
        {
            from: "*.js",
            to: '${LIB}/less',
        }
    ],
    "app": {
        "client": {
            "+scripts": [ 
                "${LIB}/less/less.js" 
            ]
        }
    }
}
