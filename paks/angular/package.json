{
    "name": "angular",
    "description": "Angular client-side framework",
    "version": "1.2.7",
    "keywords": [
        "esp",
    ],
    "export": [
        {
            from: [
                "angular.js",
                "angular-animate.js",
                "angular-route.js",
            ],
            to: '${LIB}/angular',
            overwrite: true,
        }
    ],
    "app": {
        "client": {
            "+scripts": [
                "${LIB}/angular/angular.js",
                "${LIB}/angular/angular-animate.js",
                "${LIB}/angular/angular-route.js",
            ]
        }
    }
}
