{
    "name": "respond",
    "description": "Polyfill for CSS media queries",
    "version": "1.1.0",
    "keywords": [
        "esp",
    ],
    "export": [
        {
            from: "*.js",
            to: '${LIB}/respond',
            overwrite: true,
        }
    ],
    "app": {
        "client": {
            "+scripts": [ 
                "${LIB}/respond/respond.js" 
            ]
        }
    }
}
