{
    "name": "esp-gui",
    "description": "ESP GUI Extensions for Angular",
    "version": "5.0.0-rc2",
    "keywords": [
        "esp",
    ],
    "dependencies": {
        "esp-best": "~5.0",
    },
    "licenses": [
        {
            "type": "Commercial",
            "url": "http://embedthis.com/licensing/"
        }
    ],
    "export": [
        {
            from: [
                "*.js",
            ],
            to: "${LIB}/esp-gui",
            overwrite: true,
        }
    ],
    "app": {
        "client": {
            "+scripts": [ 
                "${LIB}/esp-gui/*.js" 
            ],
        },
    },
    "ignore": [
        "doc/**",
        "start.me",
    ],
}
