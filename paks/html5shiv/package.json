{
    name: "html5shiv",
    "description": "Javascript polyfill for IE 8 and before",
    "version": "3.6.2",
    "keywords": [
        "esp",
    ],
    "export": [
        {
            from: "*.js",
            to: '${LIB}/html5shiv',
        }
    ],
    "app": {
        "client": {
            "+scripts": [ 
                "${LIB}/html5shiv/html5shiv.js" 
            ]
        }
    }
}
