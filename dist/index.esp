<!DOCTYPE html>
<html lang="en" ng-csp>
<head>
    
    <title>Kickstart</title>
    <meta charset="utf-8" />
    <meta name="description" content="Kickstart Appweb / ESP Demonstration Application">
    <link rel="icon" href="./images/favicon.ico" type="image/x-icon">
    <link href="lib/angular/angular-csp.css" rel="stylesheet" type="text/css" />
    <link href="css/all.css" rel="stylesheet" type="text/css" />
    

</head>
<body id='body' ng-controller="HomeControl" data-config='<% renderConfig(); %>'>
    <header>
        
        <nav class="navbar navbar-inverse navbar-static-top">
            <div class="navbar-header">
                <a class="navbar-brand ng-cloak" href="#/"><i class="glyphicon glyphicon-tower"></i> Kickstart</sup></a>
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav navbar-right ng-cloak">
                    <li>
                        <form class="navbar-form">
                            <input class="form-control" type="text">
                            <i class="glyphicon glyphicon-search"></i>
                        </form>
                    </li>
                    <li ng-if="Esp.can('edit')"><a href="#/setting/"><span class="glyphicon glyphicon-wrench"></span></a></li>
                    <li class="dropdown ng-cloak" ng-controller="StatusControl" ng-show="events.length">
                        <a href="#" class="dropdown-toggle"><i class="fa fa-warning"></i></a>
                        <ul class="dropdown-menu notify">
                            <li class="header">You have {{eventCount}} event notices</li>
                            <li class="message" ng-repeat="event in events" esp-click="/event/{{events[$index].id}}">
                                <i class="severity fa fa-inverse event-{{event.type}}" ng-class="Esp.faclass(event.type)"></i>
                                <span>{{event.title}}</span>
                                <span class="time"><i class="fa fa-clock-o"></i> {{event.date}}</span>
                            </li>
                            <li class="footer"><a href="#/event/list">View all events</a></li>
                        </ul>
                    </li>
                    <li ng-if="Esp.can('edit')" class="dropdown" ng-controller="UserControl">
                        <a href="#" class="dropdown-toggle" ng-cloak>{{authname()}} 
                            <b class="caret"></b>
                        </a>
                        <ul class="dropdown-menu">
                            <li>
                                <a ng-href="#/user/{{Esp.user ? 'logout' : 'login'}}">
                                    <i class="fa fa-power-off"></i> {{Esp.user ? 'Sign out' : 'Sign in'}}
                                </a>
                            </li>
                        </ul>
                    </li>
                    <li ng-if="!Esp.can('edit')" ng-controller="UserControl">
                        <a ng-href="#/user/login">
                            <i class="fa fa-power-off"></i>
                        </a>
                    </li>
                    <li><a href=""><i class="fa fa-question"></i></a></li>
                </ul>
            </div>
        </nav>

    </header>
    <main class="container" ng-cloak>
        
        <nav class="sidebar nav-collapse col-sm-2" ng-show="showNav">
            <ul class="nav nav-tabs nav-stacked">
                <esp-menu href="/" value="Dash" icon="fa fa-tachometer"></esp-menu>
                <esp-menu href="/port/list" value="Ports" icon="fa fa-sitemap"></esp-menu>
                <esp-menu href="/vlan/list" value="VLANs" icon="glyphicon glyphicon-hdd"></esp-menu>
                <esp-menu ng-show="Esp.can('edit')" href="/setting" value="Settings" icon="glyphicon glyphicon-cog"></esp-menu>
                <esp-menu href="/user/list" value="Users" icon="glyphicon glyphicon-user"></esp-menu>
                <esp-menu href="/event/list" value="Events" icon="glyphicon glyphicon-list"></esp-menu>
            </ul>
        </nav>


        <section class="content col-sm-10" id="content">
            <div ng-view class="ng-cloak"></div>
        </section>

    </main>
    
    <div ng-repeat="(field, msg) in feedback">
        <span class="ng-hide-remove" ng-class="'feedback-' + field" ng-cloak>{{msg}}</span>
    </div>
    <footer id="author" ng-cloak>
        <span class="pull-right">Powered by ESP &mdash; &copy; Embedthis Software</span>
    </footer>
    <script src="lib/jquery/jquery.js"></script>
    <script src="lib/angular/angular.js"></script>
    <script src="lib/angular-route/angular-route.js"></script>
    <script src="lib/angular-animate/angular-animate.js"></script>
    <script src="lib/esp-angular/esp-click.js"></script>
    <script src="lib/esp-angular/esp-field-errors.js"></script>
    <script src="lib/esp-angular/esp-fixnum.js"></script>
    <script src="lib/esp-angular/esp-format.js"></script>
    <script src="lib/esp-angular/esp-input-group.js"></script>
    <script src="lib/esp-angular/esp-input.js"></script>
    <script src="lib/esp-angular/esp-resource.js"></script>
    <script src="lib/esp-angular/esp-session.js"></script>
    <script src="lib/esp-angular/esp-sockets.js"></script>
    <script src="lib/esp-angular/esp-titlecase.js"></script>
    <script src="lib/esp-angular/esp.js"></script>
    <script src="lib/html5shiv/html5shiv-printshiv.js"></script>
    <script src="lib/html5shiv/html5shiv.js"></script>
    <script src="lib/angular-bootstrap/ui-bootstrap-tpls.js"></script>
    <script src="lib/esp-angular-best/js/esp-angular-best.js"></script>
    <script src="lib/esp-angular-gui/esp-angular-gui.js"></script>
    <script src="lib/esp-angular-gui/esp-confirm.js"></script>
    <script src="lib/esp-angular-gui/esp-gauge.js"></script>
    <script src="lib/esp-angular-gui/esp-local.js"></script>
    <script src="lib/esp-angular-gui/esp-menu.js"></script>
    <script src="lib/esp-angular-gui/esp-modal.js"></script>
    <script src="lib/esp-angular-gui/esp-svg-gauge.js"></script>
    <script src="main.js"></script>
    <script src="dash/Dash.js"></script>
    <script src="dash/DashControl.js"></script>
    <script src="demo/DemoControl.js"></script>
    <script src="event/Event.js"></script>
    <script src="event/EventControl.js"></script>
    <script src="home/HomeControl.js"></script>
    <script src="port/Port.js"></script>
    <script src="port/PortControl.js"></script>
    <script src="setting/Setting.js"></script>
    <script src="setting/SettingControl.js"></script>
    <script src="status/Status.js"></script>
    <script src="status/StatusControl.js"></script>
    <script src="table/Table.js"></script>
    <script src="table/TableControl.js"></script>
    <script src="user/User.js"></script>
    <script src="user/UserControl.js"></script>
    <script src="vlan/Vlan.js"></script>
    <script src="vlan/VlanControl.js"></script>
    

</body>
</html>

