{
    "meta": {
        "url": "https://embedthis.com/appweb/demo/"
    },
    "control": {
        "post": [
            "ng-package",
            "compile-esp",
            "compress"
        ]
    },
    "services": {
        "clean-css": true
    },
    "debug": {
        "services": {
            "compress": false,
            "ng-compile-html": false,
            "ng-package": false
        }
    },
    "release": {
        "services": {
            "compress": false,
            "minify-css": {
                "minify": true
            },
            "ng-compile-js": {
                "files": [
                    "contents/**.js",
                    "lib/esp*/**.js",
                    "lib/angular*/**.js"
                ]
            },
            "ng-package": true,
            "render-js": {
                "usemap": false
            },
            "minify-js": {
                "usemap": false
            },
            "render-css": {
                "usemap": false,
                "usemin": true
            }
        }
    }
}
