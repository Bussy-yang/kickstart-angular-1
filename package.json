{
    "name": "kickstart",
    "title": "Kickstart",
    "description": "Kickstart Appweb / ESP Demonstration Application",
    "version": "0.8.1",
    "keywords": [
        "appweb",
        "esp"
    ],
    "repository": {
        "type": "git",
        "url": "git://github.com/embedthis/kickstart.git"
    },
    "dependencies": {
        "bootstrap": "~3.3",
        "esp-angular": "~5.4",
        "esp-angular-skeleton": "~5.4",
        "esp-angular-mgr-skeleton": "~5.4",
        "exp-esp": "~0.1",
        "exp-gzip": "~0.2",
        "font-awesome": "~4.3"
    },
    "pak": {
        "mode": "release",
        "import": true,
        "render": {
            "js": [
                "contents/main.js",
                "contents/dash/Dash.js",
                "contents/dash/DashControl.js",
                "contents/demo/DemoControl.js",
                "contents/event/Event.js",
                "contents/event/EventControl.js",
                "contents/home/HomeControl.js",
                "contents/main.js",
                "contents/port/Port.js",
                "contents/port/PortControl.js",
                "contents/setting/Setting.js",
                "contents/setting/SettingControl.js",
                "contents/status/Status.js",
                "contents/status/StatusControl.js",
                "contents/table/Table.js",
                "contents/table/TableControl.js",
                "contents/user/User.js",
                "contents/user/UserControl.js",
                "contents/vlan/Vlan.js",
                "contents/vlan/VlanControl.js"
            ]
        },
        "overrides": {
            "bootstrap": {
                "pak": {
                    "export": [
                        "less/**",
                        {
                            "from": [
                                "dist/fonts/*.eot",
                                "dist/fonts/*.woff*",
                                "dist/fonts/*.ttf"
                            ],
                            "trim": 1
                        }
                    ]
                }
            },
            "font-awesome": {
                "pak": {
                    "export": [
                        "less/**",
                        {
                            "from": [
                                "fonts/*.eot",
                                "fonts/*.woff*",
                                "fonts/*.ttf"
                            ]
                        }
                    ]
                }
            }
        }
    },
    "directories": {
        "dist": "dist",
        "controllers": "controllers"
    },
    "devDependencies": {
        "esp": "~6.x"
    }
}
